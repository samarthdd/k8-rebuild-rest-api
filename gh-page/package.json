{
  "name": "glasswall-rebuild",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.41",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.2.4",
    "@types/swagger-ui-react": "^3.23.1",
    "copy-to-clipboard": "^3.3.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "react-toastify": "^6.0.5",
    "react-transition-group": "^4.3.0",
    "swagger-ui-react": "^3.25.0",
    "typescript": "^3.7.5"
  },
  "homepage": "http://filetrust.github.io/cloudsdk-aws-rebuild-api",
  "scripts": {
    "start": "npm-run-all -p start:react start:scss",
    "start:react": "react-scripts start",
    "start:scss": "npm-run-all -p build:scss watch:scss",
    "watch:scss": "node-sass --include-path ./src --include-path ./node_modules src/sass -o src/ --watch --recursive",
    "build": "npm-run-all -p build:react build:scss",
    "build:react": "react-scripts build",
    "build:scss": "node-sass --include-path ./src/ --include-path ./node_modules src/sass -o src/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "cypress": "npm-run-all -p cypress:start-server cypress:open",
    "cypress:start-server": "npm run start",
    "cypress:open": "node_modules/.bin/cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^4.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5"
  }
}
